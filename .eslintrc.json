{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:cypress/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "consistent-return": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": 0,
    "import/no-extraneous-dependencies": 0,
    "jsx-a11y/href-no-hash": [0]
  }
}
