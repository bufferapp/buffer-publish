{
  "name": "@bufferapp/publish-composer",
  "version": "2.0.0",
  "description": "The publish web composer and its interfaces",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9001 -c ./composer/.storybook",
    "lint": "echo \"disabled command: eslint . --ignore-pattern coverage node_modules\"",
    "test": "yarn run lint && jest",
    "test-watch": "jest --watch",
    "test-update": "jest -u",
    "test-without-coverage": "yarn run lint && jest --coverage=false --collectCoverage=false",
    "compile": "rimraf dist/* && webpack --output-path=dist --config webpack.prod.config.js",
    "compile-dev": "rimraf dist/* && webpack --output-path=dist"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testURL": "https://local.buffer.com",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": ["./composer/tests/setupTests.js"]
  },
  "author": "pioul06+github@gmail.com",
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "autoprefixer-loader": "^3.2.0",
    "css-loader": "^0.28.11",
    "element-closest": "^2.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "exports-loader": "^0.6.4",
    "find-with-regex": "^1.0.2",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "24.9.0",
    "js-cookie": "^2.2.0",
    "micro-rpc-client": "^0.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-calc": "5.2.0",
    "postcss-color-function": "2.0.1",
    "postcss-custom-properties": "5.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "0.8.2",
    "pusher-js": "^4.2.1",
    "style-loader": "^0.19.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bufferapp/buffer-js-api": "0.3.0",
    "@bufferapp/buffer-js-metrics": "0.2.0",
    "@bufferapp/buffer-js-request": "0.2.0",
    "@bufferapp/components": "3.0.2",
    "@bufferapp/draft-js": "0.10.1-fork.5",
    "@bufferapp/draft-js-emoji-plugin": "^2.0.2",
