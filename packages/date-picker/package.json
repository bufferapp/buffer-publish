    }
  },
  "author": "mike@msanroman.io",
  "dependencies": {
    "@bufferapp/analyze-profile-selector": "1.11.0",
    "@bufferapp/async-data-fetch": "1.9.4",
    "@bufferapp/components": "3.2.1",
    "moment": "^2.18.1",
    "react-tooltip": "^3.3.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "19.0.2",
    "storybook-addon-a11y": "3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
